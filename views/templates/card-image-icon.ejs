<!-- Should create card image icons from variables-->
<div id="cardImageIcon">
    
    <svg width="125" height="110" xmlns="http://www.w3.org/2000/svg">
        
       <defs>
          <clipPath id = "box">
              <rect x="0" y="0" width="125" height="110" rx="7" />
          </clipPath>
       </defs>
       
             
       <image clip-path="url(#box)" href='<%=image%>' x="-25" y="-75" height="275" width="200" />
       
       </svg>

       <div id="cardTextDiv">
       <span><%=text%></span>
    </div>    
 </div>